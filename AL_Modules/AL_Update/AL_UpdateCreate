Sub AL_UpdateCreate()

    Dim ws As Worksheet
    AL_UpdateInitialize
    For Each ws In Worksheets
        If ws.Name = "Update" Then
                AL_ErrorPrint 3, 1, "Update"
                AL_ErrorShow 3, 1, "Update"
                Exit Sub
        End If
    Next ws
    Set ws = ThisWorkbook.Sheets.Add(After:=ThisWorkbook.Sheets(ThisWorkbook.Sheets.Count))
    ws.Name = "Update"
    AL_UpdateInitialize

End Sub