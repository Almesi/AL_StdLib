Sub AL_Array_TEST_Integer()

    Dim Array1() As Integer
    Dim Test_Integer() As Integer
    Dim Test2_Integer() As Integer
    Dim i As Long

    ReDim Array1(6)
    ReDim Test_Integer(6)
    Array1(0) = Test_Integer(0)
    Array1(0) = Test_Integer(1)
    Array1(0) = Test_Integer(2)
    Array1(0) = Test_Integer(3)
    Array1(0) = Test_Integer(4)
    Array1(0) = Test_Integer(5)
    Array1(0) = Test_Integer(6)

    AL_Array_Delete_Integer Array1, 3
    If AL_Array_InArray_Integer(Array1, Test_Integer(3)) = False Then
        AL_Array_Splice_Integer Array1, Test_Integer(3), 3 
    End If
    AL_Array_Pop_Integer Array1
    AL_Array_Push_Integer Array1, Test_Integer(6)
    AL_Array_Shift_Integer Array1
    AL_Array_Unshift_Integer Array1, Test_Integer(6)
    i = AL_Array_Search_Integer(Array1, Test_Integer(3))
    Test2_Integer = AL_Array_Merge_Integer(Array1, Test_Integer)

End Sub