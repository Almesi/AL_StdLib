Sub AL_ErrorCreate()

    Dim ws As Worksheet
    AL_ErrorInitialize
    If AL_CheckSheet("Error", True) = True Then
        Set ws = ThisWorkbook.Sheets.Add(After:=ThisWorkbook.Sheets(ThisWorkbook.Sheets.Count))
        ws.Name = "Error"
        AL_ErrorInitialize
        AL_ErrorCreateBasicError
    End If

End Sub