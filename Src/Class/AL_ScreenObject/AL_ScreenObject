' Variable Declaration
    ' Sets to Const
    Private p_Initialized As Boolean
    
    ' Double Value and variable
    Private p_Vertex As New AL_Vertex

    ' Whole Value as Vertex of upper Left corner
    Private p_og_Vertex As New AL_Vertex
    
    ' Double Value and variable
    Private p_Length As New AL_Vertex

    ' Whole Value for Number of Cells in Length to Down Right Corner
    Private p_og_Length As New AL_Vertex
    
    ' Range of Texture of upper Vertexs
    Private p_Texture As Range
'

'Get Let Set 
    Public Property Let Vertex(Value As AL_Vertex)
        Set p_Vertex = Value
    End Property

    Public Property Get Vertex() As AL_Vertex
        Set Vertex = p_Vertex
    End Property

    Public Property Let Length(Value As AL_Vertex)
        Set p_Length = Value
    End Property

    Public Property Get Length() As AL_Vertex
        Set Length = p_Length
    End Property

    Public Property Let OgVertex(Value As AL_Vertex)
        Set p_OgVertex = Value
    End Property

    Public Property Get OgVertex() As AL_Vertex
        Set OgVertex = p_og_Vertex
    End Property

    Public Property Let OgLength(Value As AL_Vertex)
        Set p_Length = Value
    End Property

    Public Property Get OgLength() As AL_Vertex
        Set OgLength = p_og_Length
    End Property

    Public Property Let Texture(Value As Range)
        Set p_Texture = Value
    End Property

    Public Property Get Texture() As Range
        Set Texture = p_Texture
    End Property
'
Private Function IsInitialized()
    If p_Initialized = False Then
        AL_Error_Show AL_Error_Class, 4, "AL_ScreenObject"
    End If
    IsInitialized = True
End Function

Public Sub Initialize(Vertex_X As Double, Vertex_Y As Double, Vertex_Z As Double, Length_X As Double, Length_Y As Double, Length_Z As Double)
    If p_Initialized = False Then
        p_Vertex.X  = Vertex_X
        p_Vertex.Y  = Vertex_Y
        p_Vertex.Z  = Vertex_Z
        p_Length.X = Length_X
        p_Length.Y = Length_Y
        p_Length.Z = Length_Z
        p_og_Vertex.X  = Vertex_X
        p_og_Vertex.Y  = Vertex_Y
        p_og_Vertex.Z  = Vertex_Z
        p_og_Length.X = Length_X
        p_og_Length.Y = Length_Y
        p_og_Length.Z = Length_Z
        p_Initialized = True
    End If
End Sub