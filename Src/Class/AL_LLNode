Private p_Value As Variant
Private p_NextNode As AL_LLNode

Public Property Get Value() As Variant
    Value = p_Value
End Property

Public Property Let Value(n_Value As Variant)
    p_Value = n_Value
End Property

Public Property Get NextNode() As AL_LLNode
    Set NextNode = p_NextNode
End Property

Public Property Set NextNode(n_NextNode As AL_LLNode)
    Set p_NextNode = n_NextNode
End Property